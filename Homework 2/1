/*Вывести квадратную
матрицу по заданному N.
Пример N = 3:
1 2 3
4 5 6
7 8 9*/
#include "stdio.h"
#define N 3
int main() {
  unsigned int m=1;
  int matr[N][N];
  for(int i=0;i<N;++i) {
    for(int j=0;j<N;++j) {
      matr[i][j]=m;
      ++m;
    }
  }
  
  for(int i=0;i<N;++i) {
    for(int j=0;j<N;++j) {
      printf("%d ",matr[i][j]);
    }
    printf("\n");
  }
  return 0;
}
