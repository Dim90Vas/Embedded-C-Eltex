/*Вывести заданный массив размером N
в обратном порядке.
Пример:
Ввод –> 1 2 3 4 5
Вывод –> 5 4 3 2 1*/
#include "stdio.h"
#define N 5
int main() { 
  int n;
  int array[N];
  for(int i=0;i<N;++i) {
    scanf("%d",&array[i]);
  }
  for(int j=N-1;j>=0;--j) {
    printf("%d ",array[j]);
  }
  return 0;
}
